<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".MainActivity"
    >

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >


        <fragment
            android:id="@+id/watch_list_fragment"
            android:name="com.example.ttt0407projectnavigationapp.fragments.WatchListFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout="@layout/fragment_watch_list"
            />

    </FrameLayout>

    <!--
        class="com.example.ttt0407projectnavigationapp.fragments.WatchListFragment"
        class="com.example.ttt0407projectnavigationapp.fragments.CompanyFragment"


-->
</android.support.constraint.ConstraintLayout>